{
    "name": "spatie/laravel-tail",
    "description": "Easily tail the application log",
    "keywords": [
        "laravel",
        "tail",
        "log",
        "laravel-tail",
        "development"
    ],
    "homepage": "https://github.com/spatie/laravel-tail",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://murze.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "5.5.x-dev",
        "illuminate/console": "5.5.x-dev",
        "symfony/process": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Tail\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Tail\\TailServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
